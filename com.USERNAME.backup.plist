<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" \
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<dict>
        <key>Label</key>
        <string>com.USERNAME.backup</string>
        <key>LowPriorityIO</key>
        <true/>
        <key>Program</key>
       	  <!-- Directory you place backup.sh 
       	       Preferably a permanent location
       	  -->
          <string>/Users/isabella/Desktop/backup.sh</string>
        <key>ProgramArguments</key>
        	<array>
              	  <string>./backup.sh</string>
              	  <!-- Directory where backups are located
       	               See readme for more info
       	  	  -->
			<string>/Users/isabella/Documents/FlashTransfer/</string>
        	</array>
        <key>WatchPaths</key>
        	<array>
        		<string>/Volumes</string>
        	</array>
</dict>
</plist>
